<div class="submission-subform">
    <div class="col-lg-2 text-center">
        <span class="badge citation-item-order"></span>
    </div>
    <div class="col-lg-10">
        <div class="alert alert-success citation-box">{{ form.vars.value.citation.raw }}</div>
    </div>
    {% for element in form %}
        {{ form_row(element, {'style': 'horizontal'})  }}
        {{ form_row(element)  }}

    {% endfor %}
    <button type="button" class="btn btn-warning btn-sm submission-subform-remove-panel remove-citation"><i class="fa fa-trash"></i></button>
</div>
